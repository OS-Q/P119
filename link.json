      "OS-Q",
      "dev-platform",
      "Texas Instruments",
      "MSP430"
  ],
  "engines": {
    "platformio": "^5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OS-Q/P139.git"
  },
  "version": "2.3.0",
  "frameworks": {
    "arduino": {
