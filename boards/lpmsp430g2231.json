{
  "build": {
    "core": "msp430",
    "extra_flags": "-DENERGIA_ARCH_MSP430 -DENERGIA_MSP_EXP430G2231LP",
    "f_cpu": "1000000L",
    "hwids": [
      [
        "0x2341",
        "0x0c9f"
      ]
    ],
    "mcu": "msp430g2231",
    "variant": "MSP-EXP430G2231LP"
  },
  "debug": {
    "tools": {
      "mspdebug": {
        "onboard": true,
        "server": [
          {
            "arguments": [
              "tilib",
              "gdb"
            ],
            "executable": "mspdebug",
            "package": "tool-mspdebug",
            "system": [
              "windows",
              "windows_amd64",
              "windows_x86"
            ]
          },
          {
            "arguments": [
              "$UPLOAD_PROTOCOL",
              "gdb"
            ],
            "executable": "mspdebug",
            "package": "tool-mspdebug",
            "system": [
              "darwin_x86_64",
              "linux_i686",
              "linux_x86_64"
            ]
          }
        ]
      }
    }
  },
  "frameworks": [
    "arduino"
  ],
  "name": "TI LaunchPad MSP-EXP430G2 w/ MSP430G2231",
  "upload": {
    "maximum_ram_size": 256,
    "maximum_size": 2048,
    "protocol": "rf2500"
  },
  "url": "http://www.ti.com/product/MSP430G2231",
  "vendor": "TI"
}
